{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"max-len": 0,
		"no-console": 0,
		"operator-linebreak": 0,
		"comma-dangle": 0,
		"implicit-arrow-linebreak": 0,
		"import/no-unresolved": 0,
		"object-curly-newline": 0,
		"no-shadow": 0,
		"prefer-const": 0,
		"quote-props": 0,
		"import/extensions": 0,
		"no-tabs": 0
	}
}
